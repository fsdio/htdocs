*{
    align-content: center;
    align-items: center;
    text-align: center;
}

body{
    padding: 20px;
    margin: 20px;
	
}

td, input {
      text-align: center;
}
	
	
table{
	align-content: center;
}

h1{
		margin-top: 20px;
}
/* Header Buka */
header ul {
  list-style-type: none;
  margin: 0;
  padding: 0;
  overflow: hidden;
  background-color: #333;
  position: -webkit-sticky; /* Safari */
  position: sticky;
  top: 0;
}

/*header li{
  float: left;
}*/

header li .right{
  float: right;
}

header li .left{
  float: left;
  font-weight: bold;
  background-color: #4CAF50;
}

header li a {
  display: block;
  color: white;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
}

header li a:hover {
  background-color: #111;
}
/* Header Tutup */
